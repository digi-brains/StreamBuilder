{
    "configuration": {
        "devUrl" : "/StreamBuilder/dist",

        "source": {
            "style" : "./src/scss/**/*.scss",
            "js"    : "./src/js/**/*.js",
            "html"  : "./src/html/**/*.html",
            "img"   : "./src/img/**/*"
        },

        "destination": {
            "style" : "./dist/css",
            "js"    : "./dist/js",
            "html"  : "./dist",
            "img"   : "./dist/img" 
        }
    }
}